#!/usr/bin/env bash
# configuring ssh client

Host ubuntu
        Hostname 107.21.38.255
        User ubuntu
        IdentityFIle ~/.ssh/school
        PasswordAuthentication no

Host LB_server
	Hostname 54.89.181.175
	User ubuntu
	IdentityFile ~/.ssh/school
	PasswordAuthentication no

Hose LB_server2
	Hostname 35.153.50.233
	User ubuntu
	IdentityFile ~/.ssh/school
	PasswordAuthentication no
